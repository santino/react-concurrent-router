{
  "name": "react-concurrent-router",
  "version": "1.0.1",
  "description": "Performant routing embracing React concurrent UI patterns",
  "author": "Santino Puleio",
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/react-concurrent-router.production.min.js",
  "sideEffects": false,
  "engines": {
    "node": ">16.12.0"
  },
  "scripts": {
    "build": "rimraf dist && yarn build:cjs&& yarn build:esm && yarn build:umd && yarn build:pick",
    "build:cjs": "cross-env BABEL_ENV=cjs rollup -c",
    "build:esm": "cross-env BABEL_ENV=esm rollup -c",
    "build:umd": "cross-env BABEL_ENV=umd rollup -c",
    "build:pick": "cherry-pick --cjs-dir cjs --esm-dir esm --cwd dist ../src",
    "postbuild:pick": "rimraf dist/utils dist/createRouter dist/RouterContext rmdir",
    "format": "prettier-standard 'src/**/*.js'",
    "test": "jest --watch",
    "testonly": "jest --runInBand --verbose && bundlesize",
    "coverage": "jest --coverage",
    "postcoverage": "open-cli ./coverage/lcov-report/index.html",
    "bundlesize": "bundlesize",
    "precommit": "lint-staged",
    "prepush": "yarn testonly",
    "cleanupminified": "rimraf dist/cjs/index.min.js dist/esm/index.min.js",
    "copydistfiles": "cp -rf package.json LICENCE README.md dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/santino/react-concurrent-router.git"
  },
  "bugs": {
    "url": "https://github.com/santino/react-concurrent-router/issues"
  },
  "homepage": "https://github.com/santino/react-concurrent-router",
  "keywords": [
    "react",
    "router",
    "concurrent",
    "route",
    "routing",
    "preloading",
    "prefetching",
    "history",
    "relay"
  ],
  "bundlesize": [
    {
      "path": "./dist/cjs/index.min.js",
      "maxSize": "7.5 Kb",
      "compression": "none"
    },
    {
      "path": "./dist/cjs/index.min.js",
      "maxSize": "3 Kb"
    },
    {
      "path": "./dist/esm/index.min.js",
      "maxSize": "7 Kb",
      "compression": "none"
    },
    {
      "path": "./dist/esm/index.min.js",
      "maxSize": "3.5 Kb"
    },
    {
      "path": "./dist/umd/react-concurrent-router.production.min.js",
      "maxSize": "14.5 Kb",
      "compression": "none"
    },
    {
      "path": "./dist/umd/react-concurrent-router.production.min.js",
      "maxSize": "5.5 Kb"
    }
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ],
    "env": [
      "jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "lint-staged": {
    "*js": [
      "prettier-standard --lint"
    ],
    "./README.md": "doctoc"
  },
  "dependencies": {
    "history": "^5.2.0"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/plugin-transform-react-constant-elements": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bundlesize": "^0.18.1",
    "cherry-pick": "^0.5.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "doctoc": "^2.1.0",
    "dtslint": "^4.2.1",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "open-cli": "^7.0.1",
    "prettier-standard": "^16.3.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.65.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.4",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.11.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
